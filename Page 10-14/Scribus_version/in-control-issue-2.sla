        <PAGEOBJECT OwnPage="2" PTYPE="4" XPOS="128.347091674805" YPOS="1249.6875" WIDTH="513.070861816406" HEIGHT="451.5732421875" RADRECT="0" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="None" PCOLOR2="None" COLUMNS="2" COLGAP="28.34645669" NAMEDLST="" SHADE="100" SHADE2="100" GRTYP="0" ROT="0" PLINEART="1" PLINEEND="0" PLINEJOIN="0" LINESP="14" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" PICART="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" FLIPPEDH="0" FLIPPEDV="0" IFONT="FreeSans Regular" SCALETYPE="1" RATIO="1" PRINTABLE="1" ANNOTATION="0" ANNAME="Copy of LeftPageBodyText" TEXTFLOWMODE="1" TEXTFLOW="1" TEXTFLOW2="0" TEXTFLOW3="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" FLOP="0" PFILE="" PFILE2="" PFILE3="" PRFILE="" EPROF="" IRENDER="1" EMBEDDED="1" LOCK="0" LOCKR="0" REVERS="0" TransValue="0" TransValueS="0" TransBlend="0" TransBlendS="0" isTableItem="0" TopLine="0" LeftLine="0" RightLine="0" BottomLine="0" isGroupControl="0" NUMDASH="0" DASHS="" DASHOFF="0" NUMPO="16" POCOOR="0 0 0 0 513.071 0 513.071 0 513.071 0 513.071 0 513.071 451.573 513.071 451.573 513.071 451.573 513.071 451.573 0 451.573 0 451.573 0 451.573 0 451.573 0 0 0 0 " NUMCO="16" COCOOR="0 0 0 0 513.071 0 513.071 0 513.071 0 513.071 0 513.071 451.573 513.071 451.573 513.071 451.573 513.071 451.573 0 451.573 0 451.573 0 451.573 0 451.573 0 0 0 0 " NUMGROUP="0" GROUPS="" startArrowIndex="0" endArrowIndex="0" OnMasterPage="" ImageClip="" ImageRes="1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" ALIGN="3" LAYER="3" BOOKMARK="0" NEXTITEM="-1" BACKITEM="-1">
            <ITEXT FONT="FreeSans Bold" FONTSIZE="14" FEATURES="inherit underline" CH="Circuit Description"/>
            <para/>
            <para/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="Because the Raspberry Pi GPIO port is not protected"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="with a buffer, we have included a 1KΩ (Ohm)"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="resistor (R2) between the switch and the GPIO pin"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="as a precaution to prevent damage if the GPIO port"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="is accidentally set as an output. Our tactile switch is"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="connected to the 0V or ground line so that when it is"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="pressed the circuit is completed between the GPIO"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="pin and zero Volts."/>
            <para/>
            <para/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="The LED along with a 470Ω current limiting resistor"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="(R1) is connected to the 3.3V positive rail. Pressing"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="the switch completes the circuit between the LED"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="and 0V causing it to light up, this provides a visual"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="indication when the switch is pressed. Finally we"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="have a 10KΩ pull-up resistor (R3)."/>
            <para/>
            <para/>
            <para/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="14" FEATURES="inherit underline" CH="Digital Logic States"/>
            <para/>
            <para/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="As mentioned earlier, computers see the world as"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="a series of zeros and ones. The Raspberry Pi uses"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="3.3V CMOS logic meaning that a binary 1 is created"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="by applying a voltage of 3.3V to an input. "/>
            <para/>
            <para/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="When the"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="voltage is present it is referred to as a logic HIGH"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="state. Alternatively when the GPIO pin is connected"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="to 0V a binary 0 or logic LOW state is created. In"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="our switch example pressing the switch creates a"/>
            <ITEXT CH=" "/>
            <ITEXT FONT="FreeSans Regular" FONTSIZE="11" CH="LOW state by connecting the IO port to 0V. ►"/>
            <trail/>
            <PageItemAttributes/>
        </PAGEOBJECT>
        <PAGEOBJECT OwnPage="3" PTYPE="4" XPOS="1262.20535437244" YPOS="1715.43401576142" WIDTH="28.34645669" HEIGHT="28.34645669" RADRECT="0" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR="None" PCOLOR2="None" COLUMNS="1" COLGAP="0" NAMEDLST="" SHADE="100" SHADE2="100" GRTYP="0" ROT="0" PLINEART="1" PLINEEND="0" PLINEJOIN="0" LINESP="16" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" PICART="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" FLIPPEDH="0" FLIPPEDV="0" IFONT="FreeSans Bold Oblique" ISIZE="16" SCALETYPE="1" RATIO="1" PRINTABLE="1" ANNOTATION="0" ANNAME="Copy of RightPageNumber" TEXTFLOWMODE="0" TEXTFLOW="0" TEXTFLOW2="0" TEXTFLOW3="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" FLOP="0" PFILE="" PFILE2="" PFILE3="" PRFILE="" EPROF="" IRENDER="1" EMBEDDED="1" LOCK="1" LOCKR="1" REVERS="0" TransValue="0" TransValueS="0" TransBlend="0" TransBlendS="0" isTableItem="0" TopLine="0" LeftLine="0" RightLine="0" BottomLine="0" isGroupControl="0" NUMDASH="0" DASHS="" DASHOFF="0" NUMPO="16" POCOOR="0 0 0 0 28.3465 0 28.3465 0 28.3465 0 28.3465 0 28.3465 28.3465 28.3465 28.3465 28.3465 28.3465 28.3465 28.3465 0 28.3465 0 28.3465 0 28.3465 0 28.3465 0 0 0 0 " NUMCO="16" COCOOR="0 0 0 0 28.3465 0 28.3465 0 28.3465 0 28.3465 0 28.3465 28.3465 28.3465 28.3465 28.3465 28.3465 28.3465 28.3465 0 28.3465 0 28.3465 0 28.3465 0 28.3465 0 0 0 0 " NUMGROUP="0" GROUPS="" startArrowIndex="0" endArrowIndex="0" OnMasterPage="" ImageClip="" ImageRes="1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" ALIGN="0" LAYER="1" BOOKMARK="0" NEXTITEM="-1" BACKITEM="-1">
